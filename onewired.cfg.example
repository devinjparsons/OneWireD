[onewire]
controller = localhost:4304
basicloglevel = DEBUG

[graylog]
enable = false
loglevel = INFO

# no longer supported
[postgresql]
enable = false

[webservice]
enable = true
port = 8088

[graphite]
enable = true
namespace = onewired.temps
host = graphite
port = 2003

[redis]
